            </zeroOrMore>
            <optional>
               <choice>
                  <oneOrMore>
                     <choice>
                        <ref name="tei_model.div7Like"/>
                        <ref name="tei_model.divGenLike"/>
                     </choice>
                     <zeroOrMore>
                        <ref name="tei_model.global"/>
                     </zeroOrMore>
                  </oneOrMore>
                  <group>
                     <oneOrMore>
                        <group>
                           <ref name="tei_model.common"/>
                        </group>
                        <zeroOrMore>
                           <ref name="tei_model.global"/>
                        </zeroOrMore>
                     </oneOrMore>
                     <zeroOrMore>
                        <choice>
                           <ref name="tei_model.div7Like"/>
                           <ref name="tei_model.divGenLike"/>
