  <text xml:id="txt_text" xml:lang="pl">
   <body xml:id="txt_body">
    <div xml:id="txt_1-div" decls="#h_1-bibl">
     <ab n="p194in210of:pelcra:mowione:informal:720-3-010000343" xml:id="txt_1.1-ab">– hy hy hy</ab>
     <ab n="p195in211of:pelcra:mowione:informal:720-3-010000343" xml:id="txt_1.2-ab">– to lepiej do Włoch albo do Francji a nie do Anglii tam</ab>
     <ab n="p196in212of:pelcra:mowione:informal:720-3-010000343" xml:id="txt_1.3-ab">– a ty gdzie byłeś ? we Włoszech ?</ab>
     <ab n="p197in213of:pelcra:mowione:informal:720-3-010000343" xml:id="txt_1.4-ab">– we Włoszech</ab>
     <ab n="p198in214of:pelcra:mowione:informal:720-3-010000343" xml:id="txt_1.5-ab">– bardzo dobre winko</ab>
     <ab n="p199in215of:pelcra:mowione:informal:720-3-010000343" xml:id="txt_1.6-ab">– nie ? łatwo wchodzi</ab>
     <ab n="p200in216of:pelcra:mowione:informal:720-3-010000343" xml:id="txt_1.7-ab">– gorzej schodzi</ab>
     <ab n="p201in217of:pelcra:mowione:informal:720-3-010000343" xml:id="txt_1.8-ab">– y hym hym hym</ab>
     <ab n="p202in218of:pelcra:mowione:informal:720-3-010000343" xml:id="txt_1.9-ab">– y hym hyum</ab>
     <ab n="p203in219of:pelcra:mowione:informal:720-3-010000343" xml:id="txt_1.10-ab">– powiem wam że co drugi znajomy z naszego towarzystwa wyjechał albo do Anglii albo do Holandii albo do Szwajcarii</ab>
    </div>
   </body>
  </text>
