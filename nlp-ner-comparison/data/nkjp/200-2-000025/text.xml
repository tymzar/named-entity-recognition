  <text xml:id="txt_text" xml:lang="pl">
   <body xml:id="txt_body">
    <div xml:id="txt_1-div" decls="#h_1-bibl">
     <ab n="p381in527of:PWN_2002000025" xml:id="txt_1.1-ab">Aż wreszcie otworzysz mi ty.</ab>
     <ab n="p382in529of:PWN_2002000025" xml:id="txt_1.2-ab">I powiesz: Miły mój, witaj mi!</ab>
     <ab n="p383in530of:PWN_2002000025" xml:id="txt_1.3-ab">Tęskniłam tysiąc nocy i dni!</ab>
     <ab n="p384in533of:PWN_2002000025" xml:id="txt_1.4-ab">Niech żyje ziemia!</ab>
     <ab n="p385in534of:PWN_2002000025" xml:id="txt_1.5-ab">Niech żyje niebo!</ab>
     <ab n="p386in535of:PWN_2002000025" xml:id="txt_1.6-ab">I ty, co tęsknisz, żyj!</ab>
     <ab n="p387in537of:PWN_2002000025" xml:id="txt_1.7-ab">Niech żyje ziemia!</ab>
    </div>
   </body>
  </text>
