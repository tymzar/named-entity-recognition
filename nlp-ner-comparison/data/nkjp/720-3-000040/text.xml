  <text xml:id="txt_text" xml:lang="pl">
   <body xml:id="txt_body">
    <div xml:id="txt_1-div" decls="#h_1-bibl">
     <ab n="p110in118of:pelcra:pelcra_xml:pelcra_recording:82209" xml:id="txt_1.1-ab">– o Jezus. o Jezus..</ab>
     <ab n="p111in119of:pelcra:pelcra_xml:pelcra_recording:82209" xml:id="txt_1.2-ab">– a w maju te święta majowe rozumiem że nie pracujemy w przerwy..</ab>
     <ab n="p112in120of:pelcra:pelcra_xml:pelcra_recording:82209" xml:id="txt_1.3-ab">– no chyba tak. znaczy wie nie wiem nic nie wiem na temat ale pewnie nie..</ab>
     <ab n="p113in121of:pelcra:pelcra_xml:pelcra_recording:82209" xml:id="txt_1.4-ab">– ale pewnie na pewno..</ab>
     <ab n="p114in122of:pelcra:pelcra_xml:pelcra_recording:82209" xml:id="txt_1.5-ab">– ale pewnie na pewno..</ab>
     <ab n="p115in123of:pelcra:pelcra_xml:pelcra_recording:82209" xml:id="txt_1.6-ab">– to maj ładnie zleci a dziewiąty nie wiesz jaki to jest dzień tygodnia?</ab>
     <ab n="p116in124of:pelcra:pelcra_xml:pelcra_recording:82209" xml:id="txt_1.7-ab">– to jest chyba..</ab>
     <ab n="p117in125of:pelcra:pelcra_xml:pelcra_recording:82209" xml:id="txt_1.8-ab">– czekaj pierwszy jest wtorek..</ab>
     <ab n="p118in126of:pelcra:pelcra_xml:pelcra_recording:82209" xml:id="txt_1.9-ab">– czekaj ósmy wtorek. wtorek..</ab>
    </div>
   </body>
  </text>
