    <div xml:id="txt_1-div" decls="#h_1-bibl">
     <ab n="p159in167of:pelcra:mowione:informal:720-3-010000166" xml:id="txt_1.1-ab">– Gieremka</ab>
     <ab n="p160in168of:pelcra:mowione:informal:720-3-010000166" xml:id="txt_1.2-ab">– Bure u Gieremka ?</ab>
     <ab n="p161in169of:pelcra:mowione:informal:720-3-010000166" xml:id="txt_1.3-ab">– tak</ab>
     <ab n="p162in170of:pelcra:mowione:informal:720-3-010000166" xml:id="txt_1.4-ab">– tak na Gieremka</ab>
     <ab n="p163in171of:pelcra:mowione:informal:720-3-010000166" xml:id="txt_1.5-ab">– no bo Gieremek to jest dziecko z holokaustu żydowskie</ab>
     <ab n="p164in172of:pelcra:mowione:informal:720-3-010000166" xml:id="txt_1.6-ab">– no bo on jest żyd przecież</ab>
     <ab n="p165in173of:pelcra:mowione:informal:720-3-010000166" xml:id="txt_1.7-ab">– uratowane dziecko ale ale on był ochrzczony bo jego z getta uratowali Polacy</ab>
     <ab n="p166in174of:pelcra:mowione:informal:720-3-010000166" xml:id="txt_1.8-ab">– tak ?</ab>
     <ab n="p167in175of:pelcra:mowione:informal:720-3-010000166" xml:id="txt_1.9-ab">– no ale on jest. nie wiem ile tych synów było ? dwóch ?</ab>
     <ab n="p168in176of:pelcra:mowione:informal:720-3-010000166" xml:id="txt_1.10-ab">– jeden był chyba je jeden ino bo</ab>
     <ab n="p169in177of:pelcra:mowione:informal:720-3-010000166" xml:id="txt_1.11-ab">– a ten drugi to był chyba wnuczek co tak płakał</ab>
    </div>
   </body>
  </text>
